{
  "manifestVersion": 1,
  "id": "supreme-guacamole",
  "version": "0.1.72",
  "name": "Planning poker",
  "scopes": ["vso.work_write"],
  "description": "Estimate with your team your coming work.",
  "publisher": "CindyB",
  "galleryFlags": [
    "Preview"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/2.0"
  ],
  "tags": [
    "Estimation",
    "Team"
  ],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/CindyB/supreme-guacamole"
  },
  "branding": {
    "color": "rgb(48, 197, 51)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "static/images/logo.png",
      "addressable": true
    },
    {
      "path": "static/css/app.css",
      "addressable": true
    },
    {
      "path": "static/index.html",
      "addressable": true
    },
    {
      "path": "node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "lib"
    }


  ],
  "categories": [
    "Plan and track"
  ],
  "contributions": [
    {
      "id": "cindyb.supreme-guacamole.work-hub",
      "targets": [ "ms.vss-work-web.work-hub-group" ],
      "type": "ms.vss-web.hub",
      "description": "Adds a tab for playing planning poker",
      "properties": {
        "name": "Planning poker",
        "order": 100,
        "uri": "static/index.html"
      }
    },
    {
      "id": "cindyb.supreme-guacamole.queries-hub-tab",
      "type": "ms.vss-web.tab",
      "description": "Add a group for moderating planning poker",
      "targets": [
        "ms.vss-work-web.query-tabs"
      ],
      "properties": {
        "uri": "static/index.html",
        "title": "Planning poker",
        "name": "Planning poker"
      }
    },
    {
      "id": "cindyb.supreme-guacamole.work-item-form-group",
      "type": "ms.vss-work-web.work-item-form-group",
      "description": "Add a group for moderating planning poker",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "name": "Planning poker",
        "uri": "static/index.html",
        "height": 600
      }

    }
  ]
}