{
    "manifestVersion": 1,
    "id": "planning-poker",
    "version": "0.0.0",
    "name": "Planning poker",
    "description": "Extension for planning poker",
    "publisher": "CindyB",
    "links": {
        "learn": {
            "uri": "https://github.com/CindyB/supreme-guacamole"
        }
    },
    "icons": {
        "default": "images/fabrikam-logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    }, 
    "branding": {
        "color": "rgb(48, 197, 51)",
        "theme": "dark"
    },       
    "files": [
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "index.html",
            "addressable": true
        },        
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "css",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true,
            "packagePath": "scripts"
        }
    ],
    "contributions": [
        {
            "id": "queries-hub-tab",
            "type": "ms.vss-web.tab",
            "description": "Adds a tab to the Queries hub",
            "targets": [
                "ms.vss-work-web.query-tabs"
            ],
            "properties": {
                "uri": "index.html",
                "title": "Planning poker",
                "name": "Planning poker"
            }
        },
        {
            "id": "work-hub",
            "type": "ms.vss-web.hub",
            "description": "Custom hub under the Work hub group",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Planning poker",
                "order": 100,
                "uri": "index.html"
            }
        }
    ]
}
